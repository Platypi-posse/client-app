div id="brickwall">
    <img src="/images/IMAG1384.jpg" alt="" />
</div>


<div class="col-log-12 locations">
<h1>{{title}}</h1>
</div>


<div class="container picinfo">

<div class="col-lg-4 login">
<h4 class="details">location:</h4> <h4>{{location}}</h4>
<h4 class="details">art piece name:</h4> <h4>{{art_piece_name}}</h4>
<h4 class="details">description:</h4> <h4>{{description}}</h4>
<h4 class="details">intersection:</h4> <h4>{{intersection}}</h4>
<h4 class="details">directions:</h4> <h4>{{directions}}</h4>
<h4 class="details">artist:</h4> <h4>{{artist}}</h4>
</div>

<div class="col-lg-1"></div>
{{#each image}}
<div class="col-lg-7">

  <img src={{this.image}}>
  <br/>

</div>
  <br/>
{{/each}}

</div>
